#!/bin/bash

# DIRS="maxedges domsetdemands enqlog minedges dominatingset"
# DIRS="maxedges domsetdemands emrgraphstats clgraphstats"
DIRS="simulation"

if [[ $# > 0 ]]; then
    DIRS="$@"
fi

for d in $DIRS; do
    echo "Pulling $d..."
    BASENAME=`basename $d`
    DIRNAME=`dirname $d`
    rsync -avzu --delete milano:uihc/$DIRNAME/$BASENAME $HOME/uihc/$DIRNAME/
    echo "done."
done
