#!/bin/bash

case `uname -s` in
	Darwin)
		BUILDTYPE=x86-64_darwin9_gcc4.0
		;;
	Linux)
		case `uname -m` in
			i686)
				BUILDTYPE=x86_sles10_4.1
				;;
			x86_64)
				BUILDTYPE=x86-64_sles10_4.1
				;;
		esac
		;;
esac

ILOG_LICENSE_FILE=$HOME/ILOG/CPLEX_Studio_AcademicResearch122/access.ilm \
	$HOME/ILOG/CPLEX_Studio_AcademicResearch122/cplex/bin/$BUILDTYPE/cplex
